<?xml version="1.0" encoding="utf-8"?>
<orders templ="%doc_id%;%fix_dt%;%doc_no%;%user_id%;%dev_login%;%account_id%;%order_type_id%;%group_price_id%;%wareh_id%;%delivery_date%;%delivery_type_id%;%delivery_note%;%doc_note%;%payment_method_id%;%payment_delay%;%bonus%;%encashment%;%order_param_ids%;%rows%;%prod_id%;%row_no%;%pack%;%qty%;%unit_price%;%discount%;%amount%;%weight%;%volume%;">
  <![CDATA[
select 
    h.distr_id person_id,
    h.doc_id, 
    h.fix_dt, 
    h.doc_no, 
    coalesce(s2.t_id, h.user_id) user_id, 
    h.dev_login, 
    s0.t_id account_id, 
    h.doc_note, 
    h.delivery_date, 
    h.delivery_type_id, 
    h.delivery_note, 
    h.wareh_id, 
    h.group_price_id, 
    h.payment_method_id, 
    h.payment_delay, 
    h.order_type_id, 
    h.bonus, 
    h.encashment, 
    array_to_string(h.order_param_ids,',') order_param_ids, 
    h.rows, 
    t.row_no, 
    s1.t_id prod_id, 
    t.pack, 
    t.qty, 
    t.unit_price, 
    t.discount, 
    t.amount, 
    t.weight, 
    t.volume
from t_order t
    left join h_order h on h.doc_id = t.doc_id
    left join symlinks s0 on h.distr_id = s0.distr_id and h.account_id = s0.f_id and s0.obj_code = 'account'
    left join symlinks s1 on h.distr_id = s1.distr_id and t.prod_id = s1.f_id and s1.obj_code = 'product'
    left join symlinks s2 on h.distr_id = s2.distr_id and h.user_id = s2.f_id and s2.obj_code = 'user'
where h.inserted_ts::date >= current_date - 5
    and s0.t_id is not null and s1.t_id is not null
    and (select count(*) from j_docs where doc_id=h.doc_id and (ttd is null or ttd='delivered')) > 0
order by h.distr_id, h.doc_id, t.row_no
  ]]>
</orders>