<?xml version="1.0" encoding="utf-8"?>
<contacts templ="%contact_id%;%account_id%;%name%;%surname%;%patronymic%;%job_title_id%;%phone%;%mobile%;%email%;%locked%;%extra_info%;%consent%;%author_id%;">
  <![CDATA[
select 
    contact_id, account_id, name, patronymic, surname, job_title_id, phone, mobile, email, locked, extra_info, ts.guid consent, author_id 
from contacts x
    left join thumbnail_stream ts on ts.photo = x.consent
where hidden = 0
order by 1, 2
  ]]>
</contacts>