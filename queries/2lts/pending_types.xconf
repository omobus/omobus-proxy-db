<?xml version="1.0" encoding="utf-8"?>
<pending_types templ="%pending_type_id%;%descr%;%row_no%;%hidden%;">
  <![CDATA[
select 
    pending_type_id, 
    descr, 
    row_no,
    hidden
from pending_types
    where "LTS_aging_L"() <= updated_ts and updated_ts <= "LTS_aging_R"()
order by 1
  ]]>
</pending_types>