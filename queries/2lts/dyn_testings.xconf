<?xml version="1.0" encoding="utf-8"?>
<dyn_testings templ="%fix_date%;%account_id%;%contact_id%;%testing_criteria_id%;%testing_score_id%;%criteria_wf%;%score_wf%;%score%;%note%;%sla%;%fix_dt%;%user_id%;%_isrecentdata%;">
  <![CDATA[
select fix_date, account_id, contact_id, testing_criteria_id, testing_score_id, criteria_wf, score_wf, score, note, sla, fix_dt, user_id, "_isRecentData" from dyn_testings
    where "LTS_aging_L"() <= updated_ts and updated_ts <= "LTS_aging_R"()
order by 1, 2, 3, 4
  ]]>
</dyn_testings>