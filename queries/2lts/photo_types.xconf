<?xml version="1.0" encoding="utf-8"?>
<photo_types templ="%photo_type_id%;%descr%;%row_no%;%hidden%;">
  <![CDATA[
select 
    photo_type_id, 
    descr, 
    row_no,
    hidden 
from photo_types
    where "LTS_aging_L"() <= updated_ts and updated_ts <= "LTS_aging_R"()
order by 1
  ]]>
</photo_types>