<?xml version="1.0" encoding="utf-8"?>
<retail_chains templ="%rc_id%;%descr%;%ka_type%;%country_id%;%hidden%;">
  <![CDATA[
select 
    rc_id, 
    descr, 
    ka_type, 
    country_id,
    hidden
from retail_chains
    where "LTS_aging_L"() <= updated_ts and updated_ts <= "LTS_aging_R"()
order by 1
  ]]>
</retail_chains>