<?xml version="1.0" encoding="utf-8"?>
<trainings templ="%doc_id%;%fix_dt%;%user_id%;%account_id%;%training_type_id%;%contact_ids%;%tm_ids%;%doc_note%;">
  <![CDATA[
select doc_id, fix_dt, user_id, account_id, training_type_id, array_to_string(contact_ids,',') contact_ids, array_to_string(tm_ids,',') tm_ids, doc_note from h_training
    where (current_timestamp - inserted_ts) < "LTS_aging"()
order by 1
  ]]>
</trainings>