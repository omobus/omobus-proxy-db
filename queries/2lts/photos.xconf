<?xml version="1.0" encoding="utf-8"?>
<photos templ="%doc_id%;%fix_dt%;%user_id%;%account_id%;%placement_id%;%brand_id%;%photo_type_id%;%photo%;%doc_note%;%photo_param_ids%;">
  <![CDATA[
select 
    x.doc_id, x.fix_dt, x.user_id, x.account_id, x.placement_id, x.brand_id, x.photo_type_id, ts.guid photo, x.doc_note, array_to_string(x.photo_param_ids,',') photo_param_ids 
from h_photo x, thumbnail_stream ts
    where "LTS_aging_L"() <= x.inserted_ts and x.inserted_ts <= "LTS_aging_R"() and x.photo = ts.photo
order by 1
  ]]>
</photos>