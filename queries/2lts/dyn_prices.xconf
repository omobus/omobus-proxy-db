<?xml version="1.0" encoding="utf-8"?>
<dyn_prices templ="%fix_date%;%account_id%;%prod_id%;%price%;%promo%;%discount%;%note%;%rrp%;%fix_dt%;%user_id%;%doc_id%;%_isrecentdata%;">
  <![CDATA[
select fix_date, account_id, prod_id, price, promo, discount, note, rrp, fix_dt, user_id, doc_id, "_isRecentData" from dyn_prices
    where "LTS_aging_L"() <= updated_ts and updated_ts <= "LTS_aging_R"()
order by 1, 2, 3
  ]]>
</dyn_prices>