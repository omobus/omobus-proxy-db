<?xml version="1.0" encoding="utf-8"?>
<manufacturers templ="%manuf_id%;%descr%;%competitor%;%hidden%;">
  <![CDATA[
select 
    manuf_id, 
    descr, 
    competitor, 
    hidden
from manufacturers
    where "LTS_aging_L"() <= updated_ts and updated_ts <= "LTS_aging_R"()
order by 1
  ]]>
</manufacturers>