<?xml version="1.0" encoding="utf-8"?>
<brands templ="%brand_id%;%descr%;%manuf_id%;%row_no%;%hidden%;">
  <![CDATA[
select 
    brand_id, 
    descr, 
    manuf_id, 
    row_no, 
    hidden
from brands
    where "LTS_aging_L"() <= updated_ts and updated_ts <= "LTS_aging_R"()
order by 1
  ]]>
</brands>