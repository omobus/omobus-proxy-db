<?xml version="1.0" encoding="utf-8"?>
<deletions templ="%account_id%;%user_id%;%fix_dt%;%note%;%photo%;%validator_id%;%validated%;%hidden%;">
  <![CDATA[
select account_id, user_id, fix_dt, note, null photo, validator_id, validated, hidden from j_deletions
    where (current_timestamp - updated_ts) < "LTS_aging"()
order by 1, 2, 3
  ]]>
</deletions>