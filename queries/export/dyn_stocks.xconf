<?xml version="1.0" encoding="utf-8"?>
<dyn_stocks templ="%fix_date%;%account_id%;%prod_id%;%stock%;%fix_dt%;%user_id%;">
  <![CDATA[
select fix_date, account_id, prod_id, stock, fix_dt, user_id from dyn_stocks
    where updated_ts >= current_timestamp - '5 days 00:00:00'::interval
order by 1, 2, 3
  ]]>
</dyn_stocks>