<?xml version="1.0" encoding="utf-8"?>
<dyn_testings templ="%fix_date%;%account_id%;%contact_id%;%testing_criteria_id%;%testing_score_id%;%criteria_wf%;%score_wf%;%score%;%note%;%sla%;%fix_dt%;%user_id%;">
  <![CDATA[
select fix_date, account_id, contact_id, testing_criteria_id, testing_score_id, criteria_wf, score_wf, score, note, sla, fix_dt, user_id from dyn_testings
    where updated_ts >= current_timestamp - '5 days 00:00:00'::interval
order by 1, 2, 3, 4
  ]]>
</dyn_testings>