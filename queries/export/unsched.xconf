<?xml version="1.0" encoding="utf-8"?>
<unsched templ="%doc_id%;%fix_dt%;%user_id%;%unsched_type_id%;%doc_note%;">
  <![CDATA[
select doc_id, fix_dt, user_id, unsched_type_id, doc_note from h_unsched
    where inserted_ts >= current_timestamp - '5 days 00:00:00'::interval
order by 1
  ]]>
</unsched>