<?xml version="1.0" encoding="utf-8" ?>
<quest_rowsL1>
  <![CDATA[
select unnest(country_ids) country_id, format('%s+%s',qname_id,qrow_id) fid from quest_rows where country_ids is not null and hidden = 0
    union
select null country_id, format('%s+%s',qname_id,qrow_id) fid from quest_rows where country_ids is null and hidden = 0
order by 1, 2
  ]]>
</quest_rowsL1>
