<?xml version="1.0" encoding="utf-8"?>
<prices_history templ="%fix_date%;%account_id%;%prod_id%;%price%;%promo%;%latest%;" empty="no">
  <![CDATA[ 
select fix_date, account_id, prod_id, price, promo, "_isRecentData" latest from dyn_prices
    where fix_date >= (current_date+"paramInteger"('prices_history:offset'))::date_t
order by 2, 3, 1 desc
  ]]>
</prices_history>