<?xml version="1.0" encoding="utf-8"?>
<targets templ="%target_id%;%target_type_id%;%b_date%;%e_date%;%subject%;%body%;%image%;" empty="no">
  <![CDATA[
select target_id, target_type_id, b_date, e_date, subject, body, image from targets
    where hidden = 0 and b_date<=current_date::date_t and current_date::date_t<=e_date
order by 1, 4, 3
  ]]>
</targets>