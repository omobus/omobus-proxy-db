<?xml version="1.0" encoding="utf-8"?>
<agreements1 templ="%account_id%;%placement_id%;%posm_id%;%b_date%;%e_date%;%strict%;" empty="no">
  <![CDATA[ 
select account_id, placement_id, posm_id, strict, b_date, e_date from agreements1
    where (b_date <= (current_date+15)::date_t and current_date::date_t <= e_date) or (b_date = '' and e_date = '')
order by 1, 2, 3, 5
  ]]>
</agreements1>