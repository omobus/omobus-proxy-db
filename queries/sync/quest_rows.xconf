<?xml version="1.0" encoding="utf-8"?>
<quest_rows templ="%qname_id%;%qrow_id%;%descr%;%qtype%;%extra_info%;%delim%;" empty="no">
  <![CDATA[ 
select qname_id, qrow_id, pid, ftype, descr, qtype, extra_info, case when ftype=0 then null else 1 end delim from quest_rows
    where hidden = 0
order by qname_id, row_no, qrow_id
  ]]>
</quest_rows>